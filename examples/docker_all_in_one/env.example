# Docker Registry Configuration
DOCKER_REGISTRY=docker.io

# Controller Configuration
CONTROLLER_IMAGE=aenv-controller:latest

# API Service Configuration
API_SERVICE_IMAGE=aenv-api-service:latest

# EnvHub Configuration (optional, for full feature support)
ENVHUB_ENDPOINT=http://envhub:8081

# Container Cache TTL (seconds)
CACHE_TTL_SECONDS=1800

# Docker Network Name
DEFAULT_NETWORK=aenv-network
